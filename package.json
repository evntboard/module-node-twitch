{
  "name": "module-node-twitch",
  "version": "1.0.0",
  "description": "Module twitch for Evntboard",
  "author": "EvntBoard.io",
  "license": "MIT",
  "module": "src/main.js",
  "bin": "src/main.js",
  "scripts": {
    "dev": "nodemon --watch \"src/**/*.js\" --exec \"babel-node\" src/main.js",
    "start": "babel-node src/main.js",
    "prebuild": "rimraf ./dist && rimraf ./build && babel ./src --out-dir ./dist",
    "build": "cross-env PKG_CACHE_PATH=./pkg-cache node ./build.js",
    "lint": "standard --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@twurple/api": "^7.0.4",
    "@twurple/auth": "^7.0.4",
    "@twurple/chat": "^7.0.4",
    "@twurple/eventsub-ws": "^7.0.5",
    "@twurple/pubsub": "^7.0.4",
    "dotenv": "^16.3.1",
    "json-rpc-2.0": "^1.6.0",
    "prompts": "^2.4.2",
    "twurple": "^1.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.5",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "pkg": "^5.8.1",
    "pkg-fetch": "^3.5.2",
    "rcedit": "^4.0.1",
    "rimraf": "^5.0.5",
    "standard": "^17.1.0",
    "yargs": "^17.7.2"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "assets/**/*",
      "node_modules/es-get-iterator/**/*"
    ]
  }
}
